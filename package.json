{
  "name": "copy-with-line-numbers",
  "description": "Copy selected lines with line number and file path.",
  "version": "0.0.0",
  "activationCommands": {
    "atom-workspace": [
      "copy-with-line-numbers:without-path",
      "copy-with-line-numbers:with-full-path",
      "copy-with-line-numbers:with-relative-path",
      "copy-with-line-numbers:with-file-name"
    ]
  },
  "dependencies": {
    "left-pad": "^1.1.3"
  },
  "devDependencies": {
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "fixpack": "^2.3.1",
    "npm-run-all": "^3.1.2"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "keywords": [
    "copy",
    "line number",
    "path"
  ],
  "license": "MIT",
  "main": "./lib/copy-with-line-numbers",
  "repository": "https://github.com/yassh/copy-with-line-numbers",
  "scripts": {
    "fix": "npm-run-all fix:*",
    "fix:js": "eslint ./ --fix",
    "fix:pack": "fixpack",
    "lint:js": "eslint ./",
    "test": "npm-run-all lint:*"
  }
}
